{
    "indent": {
        "ReturnStatement": ">=1",
        "value": "    "
    },
    "jsx": {
        "alignWithFirstAttribute": false,
        "attrsOnSameLineAsTag": false,
        "spaceInJSXExpressionContainers": "",
        "firstAttributeOnSameLine": true,
        "formatJSX": true,
        "htmlOptions": {
            "indent_size": 4
        },
        "maxAttrsOnTag": 3
    },
    "plugins": [
        "esformatter-jsx",
        "esformatter-quotes",
        "esformatter-eol-last"
    ],
    "preset": "default",
    "quotes": {
        "avoidEscape": true,
        "type": "double"
    }
}
